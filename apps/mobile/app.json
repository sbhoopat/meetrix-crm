{
  "expo": {
    "name": "TeleCRM",
    "slug": "telecrm",
    "scheme": "telecrm",
    "version": "0.1.0",
    "orientation": "portrait",
    "sdkVersion": "52.0.0",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#05060B"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#05060B"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single"
    }
  }
}

